{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "VERCEL": "true",
    "DISABLE_CACHE": "false",
    "SHOWBOX_PROXY_URL_VALUE": "https://starlit-valkyrie-39f5ab.netlify.app/?destination=",
    "SCRAPER_MODE": "proxy",
    "USE_SCRAPER_API": "false"
  }
} 